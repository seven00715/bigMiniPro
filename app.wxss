@import './utils/icon.wxss';
@import './vender/iconfont.wxss';
/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

page{
  /* 定义主题色 */
  --themColor:#ff2d4a;
}

/* 小程序中 不支持通配符 */
page,view,text,swiper,swiper-item,image,navigator {

  padding: 0;
  margin: 0;
  box-sizing: border-box;
}

image {
  vertical-align: middle;
}


