/* 组价有独立的作用域 需要字体图标 需要自己引用 */
@font-face {
  font-family: "iconfont";
  src: url('//at.alicdn.com/t/font_1652022_xo76m2d6jjq.eot?t=1582594108143');
  /* IE9 */
  src: url('//at.alicdn.com/t/font_1652022_xo76m2d6jjq.eot?t=1582594108143#iefix') format('embedded-opentype'), /* IE6-IE8 */ url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2'), url('//at.alicdn.com/t/font_1652022_xo76m2d6jjq.woff?t=1582594108143') format('woff'), url('//at.alicdn.com/t/font_1652022_xo76m2d6jjq.ttf?t=1582594108143') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+ */ url('//at.alicdn.com/t/font_1652022_xo76m2d6jjq.svg?t=1582594108143#iconfont') format('svg');
  /* iOS 4.1- */
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.icon-xiangshang:before {
  content: "\e6b1";
}
.icon-sousuo:before {
  content: "\e752";
}
.search {
  background-color: var(--themColor);
  padding: 20rpx 16rpx;
}
.search-bar {
  background: #fff;
  height: 60rpx;
  width: 718rpx;
  border-radius: 8rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.search-bar text {
  color: #666;
  font-size: 30rpx;
  margin-left: 10rpx;
}
